{
  "configurePresets": [
    {
      "binaryDir": "${sourceDir}/build",
      "description": "Cross-platform debug build with Ninja",
      "displayName": "Default (Debug)",
      "generator": "Ninja",
      "inherits": [
        "dev",
        "debug"
      ],
      "name": "default"
    },
    {
      "binaryDir": "${sourceDir}/build",
      "description": "Cross-platform release build with Ninja",
      "displayName": "Default (Release)",
      "generator": "Ninja",
      "inherits": [
        "release"
      ],
      "name": "default-release"
    },
    {
      "errors": {
        "deprecated": true,
        "dev": true
      },
      "hidden": true,
      "name": "dev",
      "warnings": {
        "deprecated": true,
        "dev": true,
        "systemVars": true,
        "uninitialized": false,
        "unusedCli": true
      }
    },
    {
      "cacheVariables": {
        "CMAKE_CXX_COMPILER_LAUNCHER": "ccache",
        "CMAKE_C_COMPILER_LAUNCHER": "ccache"
      },
      "hidden": true,
      "name": "ccache"
    },
    {
      "cacheVariables": {
        "CMAKE_CXX_COMPILER_LAUNCHER": "sccache",
        "CMAKE_C_COMPILER_LAUNCHER": "sccache"
      },
      "hidden": true,
      "name": "sccache"
    },
    {
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "QGC_BUILD_TESTING": "ON",
        "QGC_DEBUG_QML": "ON"
      },
      "hidden": true,
      "name": "debug"
    },
    {
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "QGC_BUILD_INSTALLER": "ON",
        "QGC_BUILD_TESTING": "OFF",
        "QGC_DEBUG_QML": "OFF"
      },
      "hidden": true,
      "name": "release"
    },
    {
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "QGC_BUILD_TESTING": "ON",
        "QGC_ENABLE_COVERAGE": "ON"
      },
      "hidden": true,
      "name": "coverage"
    },
    {
      "cacheVariables": {
        "QGC_ENABLE_BLUETOOTH": "OFF",
        "QGC_ENABLE_GST_VIDEOSTREAMING": "OFF",
        "QGC_ENABLE_UVC": "OFF",
        "QGC_VIEWER3D": "OFF"
      },
      "hidden": true,
      "name": "minimal"
    }
  ],
  "testPresets": [
    {
      "hidden": true,
      "name": "default",
      "output": {
        "outputOnFailure": true
      }
    }
  ],
  "version": 6
}
