<?xml version="1.0" encoding="UTF-8"?>
<Installer>
    <Name>@CMAKE_PROJECT_NAME@</Name>
    <Version>@CMAKE_PROJECT_VERSION@</Version>
    <ReleaseDate>@QGC_APP_DATE@</ReleaseDate>
    <Title>@QGC_INSTALLER_NAME@</Title>
    <Publisher>@QGC_ORG_NAME@</Publisher>
    <ProductUrl>@CMAKE_PROJECT_HOMEPAGE_URL@</ProductUrl>
    <InstallerApplicationIcon>appicon</InstallerApplicationIcon>
    <InstallerWindowIcon>windowicon</InstallerWindowIcon>
    <Banner>installheader</Banner>
    <WizardStyle>Modern</WizardStyle>
    <StartMenuDir>@CMAKE_PROJECT_NAME@</StartMenuDir>
    <TargetDir>@HomeDir@/@CMAKE_PROJECT_NAME@</TargetDir>
    <AdminTargetDir>@RootDir@/@CMAKE_PROJECT_NAME@</AdminTargetDir>
    <MaintenanceToolName>@CMAKE_PROJECT_NAME@MaintenanceTool</MaintenanceToolName>
    <ControlScript>controller.qs</ControlScript>
    <StyleSheet>style.qss</StyleSheet>
</Installer>
