 <animation>
  <type>rotate</type>

  <object-name>shield</object-name>
  <property>controls/gear/shield</property>
  <factor>-165^												</factor>
  <center>
   <x-m>-0.169</x-m>
   <y-m>0.0</y-m>
   <z-m>1.187</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
