  </axis>
 </animation>
 <animation>
  <type>rotate</type>
  <object-name>fwheel.L</object-name>
  <object-name>fwheel2.L</object-name>
  <object-name>fscrews.L</object-name>
  <property>controls/flight/aileron</property>
  <factor>-25</factor>
  <center>
   <x-m>0.554</x-m>
   <y-m>-0.439</y-m>
