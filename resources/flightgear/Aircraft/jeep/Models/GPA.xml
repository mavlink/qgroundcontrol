  <object-name>wiper.R</object-name>
  <property>controls/gear/screen</property>
  <factor>-100</factor>
  <center>
   <x-m>1.014</x-m>
   <y-m>0.0</y-m>
   <z-m>1.175</z-m>
  </center>
  <axis>
   <x>0.0</x>
