    <model> 
      <path>Aircraft/jeep/Models/pilot.xml</path>
	  <name>pilot</name>
		<nopreview/>
      <offsets> 
        <x-m>2.1</x-m>
        <y-m>-0.32</y-m>
        <z-m>0.9</z-m>
        <pitch-deg>14</pitch-deg>
      </offsets>
    </model>
 <animation>
  <type>select</type>
  <object-name>pilot</object-name>
  <condition>
   <greater-than>
    <property>sim/current-view/view-number</property>
    <value>0.0</value>
   </greater-than>
  </condition>
 </animation>

	<model> 
      <path>Aircraft/jeep/Models/gpatrans.xml</path>
      <offsets> 
        <x-m>0.0</x-m>
        <y-m>0.0</y-m>
        <z-m>0.0</z-m>
      </offsets>
    </model>
</PropertyList>
 
