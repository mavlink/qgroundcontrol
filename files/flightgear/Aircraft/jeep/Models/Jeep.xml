<PropertyList>
 <path>jeep.ac</path>

	<offsets>
		<z-m>-0.03</z-m>
	</offsets>

  <nasal>
    <load>
      var update = aircraft.overlay_update.new();
      update.add("Aircraft/jeep/Models/AI-Liveries", "sim/model/livery/file");

    </load>
    <unload>
			print ("bye");
			loopid += 1;
    </unload>
  </nasal>


<!-- instruments -->
	<model> 
      <path>Aircraft/jeep/Models/fuelgauge.xml</path>
      <offsets> 
        <x-m>1.442</x-m>
        <y-m>0.01</y-m>
        <z-m>1.08</z-m>
      </offsets>
    </model>
	<model> 
      <path>Aircraft/jeep/Models/speedometer.xml</path>
      <offsets> 
        <x-m>1.442</x-m>
        <y-m>0.1</y-m>
        <z-m>1.023</z-m>
      </offsets>
    </model>
	<model> 
      <path>Aircraft/jeep/Models/current.xml</path>
      <offsets> 
        <x-m>1.442</x-m>
        <y-m>0.19</y-m>
        <z-m>1.08</z-m>
      </offsets>
